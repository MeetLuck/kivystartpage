<MyStartBox@MDBoxLayout>:
    orientation: 'vertical'
    padding: 2
    spacing: 2
    md_bg_color: 0,1,0,1
<TopBox@MDBoxLayout>:
    orientation: 'horizontal'
    padding: 2
    spacing: 1
    md_bg_color: 0,0,1,1
    size_hint: 1, 0.7
<BottomBox@MDBoxLayout>:
    orientation: 'vertical'
    padding: 2
    spacing: 1
    md_bg_color: 1,0,0,1
    size_hint: 1, 0.3
<TopLeftBox@MDBoxLayout>:
    orientation: 'vertical'
    padding: 2
    spacing: 1
    md_bg_color: 0,0,0.5,1
    size_hint: 0.7, 1
<TopRightBox@MDBoxLayout>:
    orientation: 'vertical'
    padding: 2
    spacing: 1
    md_bg_color: 0,0,0.7,1
    size_hint: 0.3, 1


<MyStartBox>:
    TopBox:
        TopLeftBox:
            MDBoxLayout:
                orientation:'horizontal'
                md_bg_color: 0,0.7,0,1
                size_hint: 1, 0.35
                MyCalendar:
                MDBoxLayout:
                    orientation: 'vertical'
            MDBoxLayout:
                orientation:'vertical'
                size_hint: 1, 0.65
                md_bg_color: 0,0.5,0,1
        TopRightBox: # Top Right Box
        
    BottomBox: # Bottom Box
